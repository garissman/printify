{
    "name": "garissman/printify",
    "description": "Printify PHP SDK",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ari Morales",
            "email": "arimoralesjordan@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Garissman\\Printify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garissman\\Printify\\Tests\\": "tests/"
        }
    },
    "require": {
        "spatie/laravel-data": "^4.11",
        "illuminate/http": "^11.33",
        "illuminate/support": "^11.47",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "orchestra/testbench": "^9.15"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Garissman\\Printify\\PrintifyServiceProvider"
            ]
        }
    }
}
